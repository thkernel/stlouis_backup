$("ul.errors").html("");
<% if @partner.errors.any? %>
  <% @partner.errors.full_messages.each do |message| %>
 
    $("ul.errors").append($('<div class="c-alert c-alert--danger" > ').html("<%= message.html_safe %> "));
  <% end %>
<% else %>
  $(".partners").html("<%= escape_javascript(render 'index') %>");
  $("#partner-modal").modal("hide");
 
<% end %>