<% @tables.each do |table| %>
    <tr >
        <td ><%= table.name %></td>
        
        
        <td>
					<div class="action-buttons">
					<%= link_to raw('<i class="fa fa-pencil" aria-hidden="true"></i>
'), edit_table_path(table), title: "Modifier", remote: true %>




<%= link_to raw('<i class="fa fa-trash-o" aria-hidden="true"></i>
'), table_delete_path(table), remote: true, title: "Supprimer", class: "delete" %>
</div>
</td>
    </tr>
<% end %>